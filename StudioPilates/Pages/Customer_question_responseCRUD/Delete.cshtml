@page
@model StudioPilates.Pages.Customer_question_responseCRUD.DeleteModel

@{
    ViewData["Title"] = "Excluir Respostas";
}

<h1>@ViewData["Title"]</h1>

<h3>Deseja realmente excluir esse Registro?</h3>
<div>
    <h4><b>Resposta:</b>@Model.Customer_question_response.Id_customer</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Customer_question_response.Id_customer)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Customer_question_response.Id_customer)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Customer_question_response.Id_question)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Customer_question_response.Id_question)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Customer_question_response.Response)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Customer_question_response.Response)
        </dd>
    </dl>

    <form method="post">
        <input type="hidden" asp-for="Customer_question_response.Id_customer_question_response" />
        <input type="submit" value="Excluir" class="btn btn-danger" /> |
        <a asp-page="./Index">Voltar à Listagem</a>
    </form>
</div>
